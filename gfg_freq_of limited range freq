from collections import defaultdict

class Solution:
    #Function to count the frequency of all elements from 1 to N in the array.
    def frequencyCount(self, arr, n, P):
        # code here
        dic = defaultdict(lambda : 0)
        for i in arr:
            dic[i] += 1
        for i in range(1,N+1):
            arr[i-1] = dic[i]
        return arr
