# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        l=[head.val]
        while head and head.next is not None:
            l.append(head.next.val)
            head=head.next
        return l==l[::-1]
        
